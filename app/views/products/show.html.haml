          .photo-main__item
            %img.photo-main__item--img{src: "https://static.mercdn.net/item/detail/orig/photos/m20757475153_1.jpg?1551680167v", alt: "a"}
          .photo-main__item
            %img.photo-main__item--img{src: "https://static.mercdn.net/item/detail/orig/photos/m20757475153_1.jpg?1551680167v", alt: "a"}
          .photo-main__item
            %img.photo-main__item--img{src: "https://static.mercdn.net/item/detail/orig/photos/m20757475153_1.jpg?1551680167v", alt: "a"}
        .photo-sub
          .photo-sub__item
            = image_tag 'https://static.mercdn.net/item/detail/orig/photos/m20757475153_1.jpg?1551680167v', class: "photo-sub__item--img active"
          .photo-sub__item
            = image_tag 'https://static.mercdn.net/item/detail/orig/photos/m20757475153_1.jpg?1551680167v', class: "photo-sub__item--img"
